//Numbers from 1 to 10
for(i=1;i<=10;i++)
{
    console.log(i);
}

// Even Numbers  from 1 to 25

for(i=1;i<=25;i++)
{
    if(i%2==0)
    {
        console.log(i);
    }
}

//Multiples of 5 in table format
var sum =5
for(i=1;i<=10;i++)
{
    console.log("5","x",i,"=",i*sum)
}
//Reversed string
var a="Nithesh Kumar"
var b=a.split("")
var c=b.reverse()
var d=c.join("");
console.log(d)

//Addition of Array
var a=[1,2,3,4,5]
var sum=0;
for(i=1;i<=a.length;i++)
{
    sum=sum+i
}
console.log(sum)
